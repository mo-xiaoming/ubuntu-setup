{
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.signatureHelp.enabled": true,
  "java.autobuild.enabled": true,
  "java.completion.enabled": true,
  "java.format.enabled": true,
  "java.foldingRange.enabled": true,
  "java.saveActions.organizeImports": true,
  "Lua.IntelliSense.traceBeSetted": true,
  "Lua.IntelliSense.traceFieldInject": true,
  "Lua.IntelliSense.traceLocalSet": true,
  "Lua.IntelliSense.traceReturn": true,
  "Lua.hint.setType": true,
  "clangd.fallbackFlags": [
    "-std=gnu++20",
    "-Wall",
    "-Wextra",
    "-Wshadow"
  ],
  "clangd.semanticHighlighting": true,
  "semanticTokens.filetypes": ["*"],
  "clangd.checkUpdates": true,
  "clangd.inlayHints.enable": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 5000,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.floatConfig": {
    "border": true,
    "title": "diagnostic"
  },
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "hover.floatConfig": {
    "border": true,
    "title": "hover"
  },
  "languageserver": {
    "haskell": {
      "args": [
        "--lsp"
      ],
      "command": "haskell-language-server-wrapper",
      "filetypes": [
        "haskell",
        "lhaskell"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ]
    }
  },
  "lightbulb.text": {
    // nerd-font: nf-mdi-lightbulb, text used when there're code actions
    "default": "ðŸ’¡",
    // nerd-font: nf-mdi-auto_fix, text used when there're code actions and quickfix exists
    "quickfix": "ðŸ”§"
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "signature.floatConfig": {
    "border": true,
    "title": "signature"
  },
  "suggest.disableKind": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.floatConfig": {
    "border": true,
    "title": "suggest"
  },
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.inlayHints.refreshOnInsertMode": true,
  "cSpell.userWords": [
    "Hasher",
    "LLJIT",
    "MCJIT",
    "Makefiles",
    "NOLINT",
    "NOLINTNEXTLINE",
    "Smol",
    "armv",
    "brainfuck",
    "bytecode",
    "clangd",
    "cmake",
    "codegen",
    "conancenter",
    "conanfile",
    "doctest",
    "endmark",
    "fmap",
    "gnueabihf",
    "gtest",
    "hicpp",
    "libcxx",
    "libstdc",
    "mkdir",
    "nonnull",
    "nonull",
    "rparen",
    "spdlog",
    "vcpkg",
    "xiaoming"
  ],
  "clangd.path": "/usr/bin/clangd-15"
}
